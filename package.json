{
  "name": "generator-redux-saga-material",
  "version": "0.0.0",
  "description": "A bare-bones yeoman generator built from create-react-app with some additions to make it easy to kick-start and manage projects that use react redux redux-saga material-ui jss and immutable-js",
  "homepage": "https://github.com/bmosigisi/generator-redux-saga-material",
  "author": {
    "name": "bmosigisi",
    "email": "bmosigisi@gmail.com",
    "url": "https://github.com/bmosigisi"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "jss",
    "react-jss",
    "material-ui",
    "immutable-js",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "eslint": "^5.4.0",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.24.2",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "humps": "^2.0.1",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "no-undef": 2
    }
  },
  "scripts": {
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "repository": "bmosigisi/generator-redux-saga-material",
  "license": "MIT"
}
